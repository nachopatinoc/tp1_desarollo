generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Universidades {
  id     Int    @id @map("universida")
  nombre String
}

model Grados {
  id     Int    @id @map("grado")
  nombre String
}

model Facultades {
  id     Int    @id @map("facultad")
  nombre String
}

model Especialidades {
  id     Int    @id @map("especialidad")
  nombre String
}

model Localidades {
  id        Int    @id @map("codigo")
  ciudad    String
  provincia String
  paisDelC  String @map("pais_del_c")
}

model Paises {
  id     Int    @id @map("pais")
  nombre String
}

model Planes {
  id             Int     @id @map("plan")
  especialidadId Int
  nombre         String?
}

model Orientaciones {
  id             Int    @id @map("orientacion")
  especialidadId Int
  planId         Int
  nombre         String
}

model Materias {
  id             Int    @id @map("materia")
  especialidadId Int
  planId         Int
  nombre         String
  ano            Int?
}
